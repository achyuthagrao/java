import java.util.*;

class OneDarrayImp{
	public static void main(String[]args){
	int a[][][]=new int[3][3][4];
	int i,j,k;
	Scanner scan=new Scanner(System.in);
	for(i=0;i<=2;i++){
		for(j=0;j<=2;j++){
			for(k=0;k<=3;k++){
				System.out.println("enter an element");
				a[i][j][k]=scan.nextInt();                  
			}
	}
}
for(i=0;i<=2;i++){
		for(j=0;j<=2;j++){
			for(k=0;k<=3;k++){
				System.out.print(a[i][j][k]+" ");
				System.out.println();
			}
			System.out.println();
	}
System.out.println();
System.out.println();
}
}
}
